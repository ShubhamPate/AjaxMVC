@model IEnumerable<AjaxMVC.Models.tblProduct>
    <table width="500" border="1">
        @foreach(var item in Model)
        {
            <tr>
                <td>@item.ProductId</td>
                <td>@item.Name</td>
                <td>@Html.DisplayFor(m=>item.Price)</td>
            </tr>
        }
    </table>
